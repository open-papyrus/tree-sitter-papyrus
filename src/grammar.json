{
  "name": "papyrus",
  "word": "identifier",
  "rules": {
    "script_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "script_header"
        }
      ]
    },
    "script_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "ScriptName"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "namespaced_identifier"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_terminator"
        }
      ]
    },
    "single_line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n]*"
        },
        {
          "type": "SYMBOL",
          "name": "_terminator"
        }
      ]
    },
    "multi_line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ";/"
        },
        {
          "type": "PATTERN",
          "value": "(?:[^\\/]|\\/[^;])*"
        },
        {
          "type": "STRING",
          "value": "/;"
        }
      ]
    },
    "documentation_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "PATTERN",
          "value": "[^}]*"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z_0-9]*"
    },
    "namespaced_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ":"
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "_terminator": {
      "type": "STRING",
      "value": "\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\u0009|\\u000A|\\u000D|\\u0020"
    },
    {
      "type": "SYMBOL",
      "name": "single_line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "multi_line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "documentation_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

